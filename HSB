function setup() {
    createCanvas(400, 400);
    background(220);
    // HSB with H range 0-400, S range 0-400, B range 0-100:
    colorMode(HSB, 400, 400, 100);
}

function draw() {
    let YOU = random(width);
    let ME = random(height);
    fill(YOU, ME, 100);
    noStroke()
    ellipse(YOU, ME, 20);
}
